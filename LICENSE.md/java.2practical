package himani;

public class himani {
       public static void main(String[] args)
{      int x[]={1,10,6,5,10,4,2,10,10,7,8};
        int arr[];
        arr=new int[11];
        int n=x.length,count=0;
        for(int i=0;i<n;i++){
            if(x[i]%2!=0)  {
                arr[count++]=x[i];
            }
        }
        for(int i=0;i<arr.length;i++)
        {
        System.out.println(arr[i]);
        }
    }
}
